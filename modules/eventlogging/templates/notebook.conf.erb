[program:notebook]
command = ipython notebook --ipython-dir="<%= ipython_dir %>" --profile nbserver
user = <%= ipython_user %>
directory = <%= ipython_dir %>
redirect_stderr = true
stdout_logfile = /var/log/supervisor/%(program_name)s.log
stderr_logfile = NONE
environment = IPYTHONDIR=<%= ipython_dir %>
