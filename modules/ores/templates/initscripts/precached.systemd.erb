[Unit]
Description=Precached daemon
After=network-online.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=<%= @working_dir %>
ExecStart=<%= @venv_path %>/bin/python <%= @working_dir %>/submodules/ores/utility precached stream.wikimedia.org <%= @host %> --config <%= @config_dir %>

WatchdogSec=120
RestartSec=30
Restart=on-failure

[Install]
WantedBy=multi-user.target
