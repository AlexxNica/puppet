title 'VisualEditor save completion time, one-minute sliding window, last day'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1day'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'median',
           :data  => 've.mwtiming.performance.user.saveComplete.median'

field :q3, :color => 'red',
           :alias => '95th percentile',
           :data  => 've.mwtiming.performance.user.saveComplete.p95'
