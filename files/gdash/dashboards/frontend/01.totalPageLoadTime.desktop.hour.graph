title 'navigationStart to loadEventEnd on desktop sites, last hour'
linewidth 1
linemode 'connected'
hide_legend 'false'
from '-1hour'
vtitle 'milliseconds'

field :q2, :color => 'blue',
           :alias => 'median',
           :data  => 'frontend.navtiming.totalPageLoadTime.desktop.overall.median'

field :q3, :color => 'red',
           :alias => '99th percentile',
           :data  => 'frontend.navtiming.totalPageLoadTime.desktop.overall.p99'
