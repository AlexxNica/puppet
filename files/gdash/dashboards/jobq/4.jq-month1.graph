title   "Average Jobs Per Day -2months"
linewidth 1
linemode "staircase"
hide_legend "false"
from "-2month"
area "first"

field :push, :color => "green",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-pop.rate, "1day"),"Jobs Run/day"))'

field :run, :color => "blue",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-insert.rate, "1day"),"Jobs Queued/day"))'

field :pushdupl, :color => "red",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-insert-duplicate.rate, "1day"),"Duplicate jobs Queued/day"))'

field :rundupl, :color => "purple",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-pop-duplicate.rate, "1day"),"Duplicate jobs run/day"))'

field :recycle, :color => "orange",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-recycle.rate, "1day"),"Jobs recycled/day"))'

field :abandon, :color => "brown",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-abandon.rate, "1day"),"Jobs abandoned/day"))'

field :ack, :color => "aqua",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-ack.rate, "1day"),"Jobs aknowledged/day"))'
