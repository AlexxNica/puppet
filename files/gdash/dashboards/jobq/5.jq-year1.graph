title   "Average Jobs Per Day -1year"
linewidth 1
linemode "staircase"
hide_legend "false"
from "-1year"
area "first"

field :push, :color => "green",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-pop.rate, "1week"),"Jobs Run/week"))'

field :run, :color => "blue",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-insert.rate, "1week"),"Jobs Queued/week"))'

field :pushdupl, :color => "red",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-insert-duplicate.rate, "1week"),"Duplicate jobs Queued/week"))'

field :rundupl, :color => "purple",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-pop-duplicate.rate, "1week"),"Duplicate jobs run/week"))'

field :recycle, :color => "orange",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-recycle.rate, "1week"),"Jobs recycled/week"))'

field :abandon, :color => "brown",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-abandon.rate, "1week"),"Jobs abandoned/week"))'

field :ack, :color => "aqua",
               :alias => "none",
               :data  => 'cactiStyle(alias(summarize(MediaWiki.jobqueue.job-ack.rate, "1week"),"Jobs aknowledged/week"))'
