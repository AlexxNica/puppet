# Apache configuration for xenon SVG hosting.

# This file is managed by Puppet.
<VirtualHost *:80>
    DocumentRoot "/var/www"
    Alias /xenon "/srv/xenon"

    <Directory />
        AllowOverride None
    </Directory>

    <Directory /srv/xenon/svgs>
        Options Indexes
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>
</VirtualHost>
