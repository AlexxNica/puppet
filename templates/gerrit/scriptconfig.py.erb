sshuser = "<%= scope.lookupvar('gerrit::gerrit_config::gerrit_username') %>"
sshhost = "<%= scope.lookupvar('gerrit::gerrit_config::gerrit_hostname') %>"
sshport = <%= scope.lookupvar('gerrit::gerrit_config::gerrit_sshport') %>
failscore = "-1"
passscore = "+1"
failmessage = "Change did not pass lint check. Below is the output from the lint check:\n\n"
passmessage = "Lint check passed."
debug = False
logdir = "/var/lib/gerrit2/review_site/logs"
filenames = {"operations/puppet": "operations.log",
             "operations/private": "labs.log",
             "default": "operations.log"}
